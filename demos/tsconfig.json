{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "lib": [ "ES2022" ],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": false,
    "resolveJsonModule": true,
    "composite": true
  },
  "include": [
    "shared/**/*.ts",
    "01-svg-frames/**/*.ts",
    "02-vscode-test-electron/**/*.ts"
  ]
}
